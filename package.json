  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "run-script-os",
    "serve": "run-script-os",
    "start:default": "HTTPS=true PORT=443 react-app-rewired start",
    "serve:default": "HTTPS=true PORT=443 react-app-rewired start",
    "start:windows": "set HTTPS=true&&set PORT=443&&react-app-rewired start",
    "serve:windows": "set HTTPS=true&&set PORT=443&&react-app-rewired start",
    "start:vidyoconnector": "env-cmd -f ./.env.vidyoconnector npm start",
    "build:vidyoconnector": "env-cmd -f ./.env.vidyoconnector npm run build",
    "osx": "HTTPS=true PORT=443 HOST=localhost.webrtc.com react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "analyze": "node scripts/analyzeWebpackBandle.js",
    "windows": "set HOST=localhost.webrtc.com&&set PORT=443&&set HTTPS=true&&react-app-rewired start",
    "lint": "eslint --max-warnings=0 src/**/*.{js,jsx,ts,tsx}",
    "npm-audit": "yarn audit --groups dependencies",
    "prepare": "husky install scripts/.husky",
    "pre-commit": "lint-staged && npm run lint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.1",
    "husky": "^8.0.1",
    "husky-run": "^0.0.0",
    "lint-staged": "^13.0.3",
    "node-sass": "^7.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-scripts": "^5.0.1",
    "run-script-os": "^1.1.6",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "resolutions": {
    "nth-check": "^2.0.1"
}
